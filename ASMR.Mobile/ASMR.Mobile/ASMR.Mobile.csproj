  <ItemGroup>
    <Folder Include="Services\Events\" />
  </ItemGroup>
  <ProjectExtensions>
    <MonoDevelop>
      <Properties>
        <Policies>
          <StandardHeader IncludeInNewFiles="True" Text="//&#xA;// ASMR: Coffee Beans Management Solution&#xA;// Â© 2021 Pandora Karya Digital. All right reserved.&#xA;//&#xA;// Written by ${AuthorName} [${AuthorEmail}]&#xA;// at ${Date} ${Time}&#xA;//&#xA;// ${FileName}&#xA;//" />
