      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <None Remove="Resources\TitilliumWeb-Light.ttf" />
    <EmbeddedResource Include="Resources\TitilliumWeb-Light.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <None Remove="Resources\TitilliumWeb-LightItalic.ttf" />
    <EmbeddedResource Include="Resources\TitilliumWeb-LightItalic.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
